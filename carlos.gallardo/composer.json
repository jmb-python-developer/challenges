{
    "name": "cgallardo/fever-code-challenge",
    "description": "Fever Code Challenge",
    "homepage": "https://github.com/FeverCodeChallenge/carlos.gallardo",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Gallardo",
            "email": "c.gallardosalas@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fever\\": "src"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "davedevelopment/phpmig": "^1.7",
        "league/tactician": "^1.1",
        "monolog/monolog": "^1.17",
        "ramsey/uuid": "^4.1",
        "slim/slim": "^3.1",
        "symfony/console": "^5.2",
        "zendframework/zend-db": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
